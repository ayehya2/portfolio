    <script>
function toggleSection(sectionId) {
    const timeline = document.getElementById(sectionId + '-timeline');
    const button = event.target;
    
    if (timeline.classList.contains('collapsed')) {
        timeline.classList.remove('collapsed');
        timeline.classList.add('expanded');
        button.textContent = 'Hide';
    } else {
        timeline.classList.remove('expanded');
        timeline.classList.add('collapsed');
        button.textContent = 'Show';
    }
}

// Initialize sections as collapsed
document.addEventListener('DOMContentLoaded', function() {
    const timelines = document.querySelectorAll('.timeline-container');
    timelines.forEach(timeline => {
        timeline.classList.add('collapsed');
    });
});
</script>